{
    "language": "ta",
    "embedding_size": 64,
    "hidden_size": 128,
    "encoder_layers": 1,
    "decoder_layers": 1,
    "cell_type": "lstm",
    "dropout": 0.2,
    "learning_rate": 0.0005,
    "batch_size": 64,
    "n_epochs": 20,
    "clip": 1.0,
    "teacher_forcing_ratio": 0.5,
    "use_attention": true,
    "max_seq_len": 50,
    "optimizer": "rmsprop",
    "rmsprop_alpha": 0.99,
    "rmsprop_eps": 1e-8,
    "weight_decay": 0.0001,
    "scheduler": "plateau",
    "scheduler_factor": 0.5,
    "scheduler_patience": 5
}
